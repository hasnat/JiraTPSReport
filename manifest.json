{
  "name": "TPS Reports for Jira",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Generate reports from your jira activity",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["scripts/activator.js"],
    "persistent": false
  },
  "options_page": "options/options.html",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "TPS Report",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "clipboardWrite",
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "options/options.html"
  ]
}